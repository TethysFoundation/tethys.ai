{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend API project",
  "files": [
    "build"
  ],
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">= 8.10.0"
  },
  "scripts": {
